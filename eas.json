{
  "cli": {
    "version": ">= 18.0.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": false,
      "channel": "production"
    },
    "release-apk": {
      "android": {
        "buildType": "apk"
      },
      "channel": "release-apk"
    }
  },
  "submit": {
    "production": {}
  }
}
